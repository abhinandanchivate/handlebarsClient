<section class="container">
  <h1 class="large text-primary">
    Dashboard
  </h1>
  <p class="lead"><i class="fas fa-user"></i> Welcome {{user.name}}</p>
  {{#if (eq dashboard.profile null)}}
  <div> <a href="/profile/create" class="btn btn-light">Create Profile</a></div>
  {{else}}
  <div>
  {{> dashboard/dashboardaction action=actionData }}

 
  {{> dashboard/expDetails exps=exps}}
   {{> dashboard/eduDetails}}
